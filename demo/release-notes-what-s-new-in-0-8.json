                              }
                            },
                            "item_type": "stacked_area_chart",
                            "item_id": "d38",
                            "height": 3,
                            "query": "log",
                            "legend": "none",
                            "hide_zero_series": false,
                            "renderer": "flot",
                            "stack_mode": "stack"
                          }
                        ],
                        "span": 6
                      },
                      {
