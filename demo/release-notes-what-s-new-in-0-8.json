                          {
                            "title": "Linear",
                            "options": {
                              "palette": "d3category20"
                            },
                            "item_type": "stacked_area_chart",
                            "item_id": "d40",
                            "height": 3,
                            "query": "log",
                            "legend": "none",
                            "hide_zero_series": false,
                            "renderer": "flot",
                            "stack_mode": "stack"
                          }
                        ],
                        "span": 6
                      }
                    ]
                  },
                  {
                    "item_type": "heading",
                    "item_id": "d49",
                    "text": "Graphite",
                    "level": "3"
                  },
                  {
                    "item_type": "row",
                    "item_id": "d42",
                    "items": [
                      {
